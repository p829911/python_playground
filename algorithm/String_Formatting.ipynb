    "n = 17\n",
    "for i in range(1, n + 1):\n",
    "    width = len(format(n, \"b\"))\n",
    "    string = \"\"\n",
    "    for m in [\"d\", \"o\", \"X\", \"b\"]:\n",
    "        if m == \"d\":\n",
    "            string += format(i, m).rjust(width)\n",
    "        else:\n",
    "            string += \" \" + format(i, m).rjust(width)\n",
    "    print(string)"
